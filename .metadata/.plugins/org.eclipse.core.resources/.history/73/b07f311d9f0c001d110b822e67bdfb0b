	public APanel(){
		this.setBackground(Color.RED);
	}
}
class BPanel extends JPanel{
	public BPanel() {
		this.setLayout(null);
	}
}

public class Ex1{
	public static void main(String[] args) {
	JFrame jf = new JFrame();
	jf.setSize(500, 600);
	}

}
