package project719;

class Constructor{
	int num;
	String str;
	
	public Constructor() {//괄호안에 매개변수 개수, 타입, 순서가 달라야한다.
		this("안녕",10);
	}
	public Constructor(
			int a) {
		num=a;
		str="안녕";
	}
	public Constructor(String s, int a) {
		num=a;
		str=s;
	}
}



public class ConstructorEx {
	public static void main(String[] args) {
		
	}
}
