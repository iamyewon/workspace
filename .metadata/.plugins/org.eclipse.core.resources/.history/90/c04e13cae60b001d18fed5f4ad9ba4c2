package project725;

import java.awt.Color;

public class GUIEx9 extends MFrame{
	public GUIEx9() {
		//super(); 없어도 자동 호출
//		super(1000,500,Color.RED); //color는 contentPane 사용해야함.
		super(1000,500,null,MFrame.FLOW_LAYOUT);
		this.setVisible(true);
	}
	
	public static void main(String[] args) {
		new GUIEx9();
	}
}
