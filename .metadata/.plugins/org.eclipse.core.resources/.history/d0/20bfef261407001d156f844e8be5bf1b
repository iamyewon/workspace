package project719;

public class Order {
	Product product;
	User user;
	int orderNum;
	int allPrice;
	
	public Order() {
		this(product,user,orderNum,allPrice);
	}
	
	public void printOrderInfo() {
		System.out.println(product+user+orderNum+allPrice);
	}
	
	public static void main(String[] args) {
		Order o = new Order();
		o.printOrderInfo();
	}
	
	
}
