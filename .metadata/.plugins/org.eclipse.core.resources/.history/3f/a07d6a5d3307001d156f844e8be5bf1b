package project719;

public class Vending {
		
		void calcChange(String drink, int money) {
		if(drink.equals("ÄÚÄ«Äİ¶ó")) { //1000
			System.out.println(drink+" ÀÜµ· "+(money-1000));
		}else if(drink.equals("Ä¥¼º»çÀÌ´Ù")) { //900
			System.out.println(drink+" ÀÜµ· "+(money-900));
		}else if(drink.equals("Æé½ÃÄİ¶ó")) { //800
			System.out.println(drink+" ÀÜµ· "+(money-800));
		}else if(drink.equals("È¯Å¸")) { //850
			System.out.println(drink+" ÀÜµ· "+(money-850));
		}else if(drink.equals("Á¦·ÎÄÚÄ«Äİ¶ó")) { //1100
			System.out.println(drink+" ÀÜµ· "+(money-1100));
		}else if(drink.equals("»ï´Ù¼ö")) { //400
