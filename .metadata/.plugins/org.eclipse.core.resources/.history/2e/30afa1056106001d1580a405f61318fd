package firstproject3;


class BookTest{
	
	String title;
	int Price;
	boolean Stock;
	double Score;
	
	
	
	void insertTitle(String t) {
		title = t;

	}
	void insertPrice(int p) {
		Price = p;
		
	}
	void insertStock(boolean ) {
		Stock = y;
	}
	void insertScore(double ) {
		Score = s;
	}
	void showData() {
		
	}
}




public class Book {
	public static void main(String[] args) {
		BookTest b = new BookTest();
		b.insertTitle("오만과 편견");
		b.insertPrice(12000);
		b.insertStock(true);
		b.insertScore(9.5);
		b.showData();
		// 결과 - 오만과 편견, 12000, 재고 있음, 평점 9.5
	}
}