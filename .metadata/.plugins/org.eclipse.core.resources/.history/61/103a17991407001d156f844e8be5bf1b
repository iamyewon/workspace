package project719;

public class Order {
	Product product;
	User user;
	int orderNum;
	int allPrice;
	
	public Order(Product product, User user, int orderNum, int allPrice) {
		this.product=product;
		this.user=user;
		this.orderNum=orderNum;
		this.allPrice=allPrice;
	}
	
	public void printOrderInfo() {
		System.out.println(orderNum+allPrice);
	}
	
	public static void main(String[] args) {
		Order o = new Order(new Product(),new User(),2,5000);
		o.printOrderInfo();
	}
	
	
}
