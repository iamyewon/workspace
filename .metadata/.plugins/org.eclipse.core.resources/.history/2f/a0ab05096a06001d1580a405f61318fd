package firstproject3;

class Gugu{
	int xx;
	int zz;
	
	void setNum(int x, int z) {
		this.xx=x;
		this.zz=z;
		
	}
	void showData(){
		for(int i=1;i<=z;i++) {
		System.out.println(x*i);
		}
	}
}



public class Gugudan {
	public static void main(String[] args) {
		Gugu g = new Gugu();
		g.setNum(2,5); //2단을 x5까지
		g.showData();
	}
}
