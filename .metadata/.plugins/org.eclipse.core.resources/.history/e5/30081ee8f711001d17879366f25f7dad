package project82;

interface Tire{void printInfo();}

class Kumho implements Tire{

	@Override
	public void printInfo() {
		
	}
	
}

class Hankook implements Tire{

	@Override
	public void printInfo() {
		
	}
	
}

public class CastingEx {
	Hankook h = new Hankook();
	Kumho k = new Kumho();
	Tire t1 = new Hankook();
	Tire t2 = new Kumho();
}
