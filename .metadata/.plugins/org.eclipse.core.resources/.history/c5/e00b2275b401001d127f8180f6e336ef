	String champ;
	String skin;
	String skill;
	String line;
	
	LOL(){
		champ="Teemo";
		skin="Little Devil";
		skill="Noxious Trap";
		line="top";
	}
	
	LOL(){
		champ = "Zyra";
		skin = "Coven Zyra Prestige Edition";
		skill="Stranglethorns";
		line="bottom-supporter";
	}
	LOL(){
		champ = "Malphite";
		skin = "Dark Star";
		skill="Unstoppable Force";
		line="top";
		
	}
	LOL(String champ, String skin, String skill, String line){
		this.champ=champ;
		this.skin=skin;
		this.skill=skill;
		this.line=line;
		
	}
}
	
public class LOL2 {
	public static void main(String[] args) {
		
		LOL champ1 = new LOL("Yuumi","EDG Yuumi","Final Chapter","Supporter");
		LOL champ2 = new LOL("Yasuo","Spirit Blossom Yasuo","Last Breath","mid");
		LOL champ3 = new LOL("Lee Sin","Nightbringer Lee Sin","Dragon's Rage","Jungle");
		LOL champ4 = new LOL("Jhin","SKT T1 Jhin","Curtain Call","adc");
		
		
		System.out.println(champ1.champ+" "+champ1.skin+" "+champ1.skill+" "+champ1.line);
		System.out.println(champ2.champ+" "+champ2.skin+" "+champ2.skill+" "+champ2.line);
		System.out.println(champ3.champ+" "+champ3.skin+" "+champ3.skill+" "+champ3.line);
		System.out.println(champ4.champ+" "+champ4.skin+" "+champ4.skill+" "+champ4.line);
		
		}
	}

	
	
	
	
	
	
	
	
	
	
	
	
