package firstproject2;

public class MethodEx4 {
	//반환되는 타입을 써야한다 = 결과의 타입
	
	int plus(){ //메서드명은 반드시 소문자로 시작
		//내용
		return 0;
	}
	
	String str() {
		//내용
		return "a";
	}
	
	double dr() {
		//내용
		return 0.0;
	}

	
// 반환형과 return 뒤에 값은 반드시 타입이 같음.

// ===============================
// 앞전에 쓰던 void.. 
	void sss() {
		
	}
	
	
}
