package firstproject;

public class ArrayEx {
	int k;
	
	ArrayEx(){//없으면 하나 만들어줌. 내용을 수정할 일이 있으면 만들면 된다.
		
	}
	
	public static void main(String[] args) {
		int[] arr = new int [10]; // new 한다 = 생성(construct)한다.
		// 배열도 메모리 올려서 사용해야한다.
	
		ArrayEx ae = new ArrayEx(); // ArrayEx() 이런건 안보인다. 
		//하지만 가상머신이 한개 만들어준다. 
		// ==> 생성자 = constructor
		
	}
}
