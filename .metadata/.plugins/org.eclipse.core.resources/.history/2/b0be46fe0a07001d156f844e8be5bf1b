package project719;

public class User {
	String userId;
	String userName;
	String userAdress;
	String userPhoneNum;
	
	public void user(String userId) {
		
	}
	
	
	public void printUserInfo() {
		System.out.println(userId+userName+userAdress+userPhoneNum);
	}
	
	public static void main(String[] args) {
		User user = new User();
		user.userId = "yeni";
	}
	
	
}
