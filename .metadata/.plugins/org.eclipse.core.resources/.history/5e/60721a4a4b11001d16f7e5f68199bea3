package project81;

public class SmartPhoneMonitor implements Monitor {
	boolean power;
	int vol;
	String resolution;
	int brightness;
	
	@Override
	public void turnOn() {
		System.out.println("휴대폰 전원이 켜집니다.");
		power = true;
	}

	@Override
	public void turnOff() {
		System.out.println("휴대폰 전원이 꺼집니다.");
		power = false;
	}
	
}
